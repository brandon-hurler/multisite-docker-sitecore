# Project Information
COMPOSE_PROJECT_NAME=multisite-docker-sitecore
VERSION=10.2

# Registries
MICROSOFT_REGISTRY=mcr.microsoft.com/
SITECORE_DOCKER_REGISTRY=scr.sitecore.com/sxp/
SITECORE_MODULE_REGISTRY=scr.sitecore.com/sxp/modules/
SITECORE_TOOLS_REGISTRY=scr.sitecore.com/tools/

# Software Versions
SITECORE_VERSION=10.2-ltsc2019
TOOLS_VERSION=10.2-1809
SPE_VERSION=6.3-1809
SXA_VERSION=10.2-1809
HORIZON_VERSION=3.0-ltsc2019
HORIZON_ASSET_VERSION=3.0-1809

# Ports
HTTPS_PORT=443
MSSQL_PORT=14330
SOLR_PORT=8984
XCONNECT_PORT=8081
TRAEFIK_MANAGEMENT_PORT=8079

# Build
SOLUTION_BUILD_IMAGE=mcr.microsoft.com/dotnet/framework/sdk:4.8
SOLUTION_BASE_IMAGE=mcr.microsoft.com/windows/nanoserver:1809
BUILD_CONFIGURATION=debug

# Volumes
LOCAL_DEPLOY_PATH=.\deploy
LOCAL_DATA_PATH=.\data

# Hostnames
CD_HOST=cd.dockerexamples.localhost
CM_HOST=cm.dockerexamples.localhost
ID_HOST=id.dockerexamples.localhost
HRZ_HOST=hrz.dockerexamples.localhost

# Prefixes
SOLR_CORE_PREFIX_NAME=sitecore

# Traefik
TRAEFIK_IMAGE=traefik:v2.2.0-windowsservercore-1809
TRAEFIK_ISOLATION=hyperv
ISOLATION=default

# Authentication
SITECORE_ADMIN_USER=
SITECORE_ADMIN_PASSWORD=
SQL_SERVER=mssql
SQL_SA_LOGIN=sa
SQL_SA_PASSWORD=
REPORTING_API_KEY=
TELERIK_ENCRYPTION_KEY=
SITECORE_IDSECRET=
SITECORE_ID_CERTIFICATE=
SITECORE_ID_CERTIFICATE_PASSWORD=
MEDIA_REQUEST_PROTECTION_SHARED_SECRET=

# Licenses
SITECORE_LICENSE=